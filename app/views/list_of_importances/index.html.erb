<p id="notice"><%= notice %></p>

<h1>Listing List Of Importances</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @list_of_importances.each do |list_of_importance| %>
      <tr>
        <td><%= link_to 'Show', list_of_importance %></td>
        <td><%= link_to 'Edit', edit_list_of_importance_path(list_of_importance) %></td>
        <td><%= link_to 'Destroy', list_of_importance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

