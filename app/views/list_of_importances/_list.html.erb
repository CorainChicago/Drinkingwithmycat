<section class="row list_of_importance">
  <h2 class= "col-md-8 col-sm-offset-4">A List of Importance</h2>
  <div class= "col-md-8 col-sm-offset-4">
    <h3><%= list.title %></h3>
    <h4><%= list.description %></h4>
    <ul>
      <% list.items.each do |l| %>
        <li><%= l.description %></li>
      <% end %>  
    </ul>
  </div>
</section>  